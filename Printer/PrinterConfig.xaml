<UserControl x:Class="Printer.PrinterConfig"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Printer"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="400">
    <Grid>
        <Label Content="Label1" HorizontalAlignment="Left" Margin="53,31,0,0" VerticalAlignment="Top"/>
        <Label Content="Label2" HorizontalAlignment="Left" Margin="53,82,0,0" VerticalAlignment="Top"/>
        <Label Content="Label3" HorizontalAlignment="Left" Margin="53,127,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="name1" DisplayMemberPath="Value" SelectedValuePath="Value" HorizontalAlignment="Left" Margin="122,35,0,0" VerticalAlignment="Top" Width="191" SelectionChanged="name1_SelectionChanged"/>
        <ComboBox x:Name="name2" HorizontalAlignment="Left" Margin="122,82,0,0" VerticalAlignment="Top" Width="191"/>
        <ComboBox x:Name="name3" HorizontalAlignment="Left" Margin="122,127,0,0" VerticalAlignment="Top" Width="191"/>

    </Grid>
</UserControl>
